   <variable name="C_VDCU" array-size="1" type="MWT_BOOL" usage="input" connect-name="C_V&lt;car&gt;DCU&lt;door&gt;">
    <comment><![CDATA[valid bit information from status signal - this door instanc is communicating]]></comment>
   </variable>
  </variable-group>
  <variable-group name="DOORS_in">
   <variable name="XSwVer" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XSwVer">
    <comment><![CDATA[System software version; Incremented if new SW is incompatible to previous SW version or hardware.; Default : 0]]></comment>
   </variable>
   <variable name="XSwRel" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XSwRel">
    <comment><![CDATA[System software release;  Incremented if new SW has major changes or additional functions but is still compatible.; Default : 0]]></comment>
   </variable>
   <variable name="XSwUpd" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XSwUpd">
    <comment><![CDATA[System software update; Incremented if new SW with minor modifications during elaboration ; Default : 0]]></comment>
   </variable>
   <variable name="XSwEvo" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XSwEvo">
    <comment><![CDATA[System software evolution; Incremented if new SW is for testing (not validated); Default : 0]]></comment>
   </variable>
   <variable name="XHwVer" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XHwVer">
    <comment><![CDATA[System hardware version ; Basic functional changes, incompatible to previous HW versions (e.g. change in firmware, I/O). ; Default : 0]]></comment>
   </variable>
   <variable name="XHwRel" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XHwRel">
    <comment><![CDATA[System hardware release ; Minor changes. Still compatible; Default : 0]]></comment>
   </variable>
   <variable name="XLifeSign_1" array-size="1" type="MWT_INT" usage="input">
    <comment><![CDATA[System shall set this counter to zero after reset including power-up and increment it with every telegram cycle.]]></comment>
   </variable>
   <variable name="XLifeSign_2" array-size="1" type="MWT_INT" usage="input">
    <comment><![CDATA[System shall set this counter to zero after reset including power-up and increment it with every telegram cycle.]]></comment>
   </variable>
   <variable name="XDevAppMd" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XDevAppMd">
    <comment><![CDATA[Device application mode]]></comment>
   </variable>
   <variable name="XDevCfg" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XDevCfg">
    <comment><![CDATA[Device configuration]]></comment>
   </variable>
   <variable name="XMd" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XMd">
    <comment><![CDATA[Door mode. Default : 1]]></comment>
   </variable>
   <variable name="XSts" array-size="1" type="MWT_USINT" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_XSts">
    <comment><![CDATA[Door Status; Clarification of Signals:; Bit 0: Door Closed & Locked (either released or not released); Bit 1: Door is inhibited due to a PED failure; Bit 2: Door is inhibited due to SDO ; Bit 3: Door is inhibited by the Driver; Bit 4: Door is either open or closed and is released.; Bit 5: Door is not in closed position; Bit 6: Door has been unlocked using either EED or EAD; Default : 0]]></comment>
   </variable>
   <variable name="SEmEgrDevOpr" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SEmEgrDevOpr">
    <comment><![CDATA[Emergency Egress Device Operated]]></comment>
   </variable>
   <variable name="SEmAccss" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SEmAccss">
    <comment><![CDATA[Emergency Access Device Operated]]></comment>
   </variable>
   <variable name="SLkdOutOfSrvc" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SLkdOutOfSrvc">
    <comment><![CDATA[Door Locked Out of Service]]></comment>
   </variable>
   <variable name="SFltA" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SFltA">
    <comment><![CDATA[Door Fault - priority A; Summary fault signal in case any priority A diagnostic code is set in the EDCU; Used as the signal to inform the driver when to isolate the door from opening]]></comment>
   </variable>
   <variable name="SFltB" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SFltB">
    <comment><![CDATA[Door Fault - priority B; Summary fault signal in case any priority B diagnostic code is set in the EDCU]]></comment>
   </variable>
   <variable name="SOpSqAct" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SOpSqAct">
    <comment><![CDATA[Door Opening sequence active.]]></comment>
   </variable>
   <variable name="SClSwAct" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SClSwAct">
    <comment><![CDATA[Door Closing sequence active.]]></comment>
   </variable>
   <variable name="STrnAtSpdFb" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_STrnAtSpdFb">
    <comment><![CDATA[Feedback of Input 3; (Train at Speed)]]></comment>
   </variable>
   <variable name="SRelFb" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SRelFb">
    <comment><![CDATA[Feedback of Input 4; (Door Release)]]></comment>
   </variable>
   <variable name="SClFb" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SClFb">
    <comment><![CDATA[Feedback of Input 5; (Door Close)]]></comment>
   </variable>
   <variable name="SOpPb" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SOpPb">
    <comment><![CDATA[Door Open Pushbutton]]></comment>
   </variable>
   <variable name="SObstDet" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SObstDet">
    <comment><![CDATA[Obstacle detected ; Signal is set TRUE as soon as an obstacle is detected either during opening or closing sequence ; Signal is reset to FALSE when door moves to Closed & Locked position.]]></comment>
   </variable>
   <variable name="SBurninAct" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SBurninAct">
    <comment><![CDATA[Burn In test (door cycling) is active.]]></comment>
   </variable>
   <variable name="SBurninOk" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SBurninOk">
    <comment><![CDATA[Burn In test (door cycling) passed.; Last Burn in test was successful: door cycling completed without any failure detected.]]></comment>
   </variable>
   <variable name="SBurninNotOk" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SBurninNotOk">
    <comment><![CDATA[Burn In test (door cycling) not passed.; Last Burn in test was not successful: door cycling not completed correctly (aborted or door fault detected).]]></comment>
   </variable>
   <variable name="SCrewSwOp" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SCrewSwOp">
    <comment><![CDATA[Crew key switch open is operated. Signal type: Steady signal.]]></comment>
   </variable>
   <variable name="SClLckFlt" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SClLckFlt">
    <comment><![CDATA[Door leaves the closed & locked position without permission.]]></comment>
   </variable>
   <variable name="SSrvcPbFlt" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SSrvcPbFlt">
    <comment><![CDATA[Service Button on the DCU fails for a duration of 1 minute.]]></comment>
   </variable>
   <variable name="SBusComFlt" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SBusComFlt">
    <comment><![CDATA[Bus communication fails ; Signals and functions via bus are no longer available]]></comment>
   </variable>
   <variable name="SSdoEnFlt" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SSdoEnFlt">
    <comment><![CDATA[SDO Enable Hardware & SDO Enable Software Mismatch 1; H/W SDO Enable =1 (I01) & S/W SDO Enable = 0 for 5s; OR; H/W SDO Enable =0 (I01) & S/W SDO Enable = 1 for 5s]]></comment>
   </variable>
   <variable name="SClFlt" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SClFlt">
    <comment><![CDATA[Door Closed HW & Door Close SW commands mismatch; - H/W close input (I05) changes to TRUE and S/W close signal is not detected TRUE within 5sec; OR;  - S/W close signal changes to TRUE and H/W close input (I05) is not detected TRUE within 5sec]]></comment>
   </variable>
   <variable name="SOpPosFlt" array-size="1" type="MWT_BOOL" usage="input" connect-name="CI_&lt;car&gt;DCU&lt;door&gt;_SOpPosFlt">
    <comment><![CDATA[System does not detect opened position within a defined time]]></comment>
   </variable>
   <variable name="SClPosFlt" array-size="1" type="MWT_BOOL" usage="input">
    <comment><![CDATA[System does not detect closed position within a defined time]]></comment>
   </variable>
   <variable name="STrnCtrlFlt" array-size="1" type="MWT_BOOL" usage="input">
    <comment><![CDATA[SIgnals from the Train Control are faulty; Door release signal active (I04) and Train at speed]]></comment>
   </variable>
