PROGRAM PAD_P_DCU_IN

(*Group:Local*)


VAR
	EnSigNeedToBeSkip :	BOOL := TRUE;
	XStsW :	WORD;(*XSts converted to WORD for convenient use of GetBitWord*)
	_XLifeSign_1 :	INT;(*local storage of lifesign1*)
	_XLifeSign_2 :	INT;(*local storage of lifesign2*)
	TOF_Lifesign1 :	TOF;(*delay for lifesign1 detection*)
	TOF_Lifesign2 :	TOF;(*delay for lifesign2 detection*)
	PTLifeSign :	TIME := TIME#500ms;(*Delay for lifesign detection*)
	PDcuLifeSignCheck :	BOOL := TRUE;(*Flag to disable the lifesign check*)
	SDcuValid :	BOOL;
END_VAR


(*Group:IN validity*)


VAR_INPUT
	Enable_FA :	BOOL;(*Enable PAD FA*)
	C_VDCU :	BOOL;(*valid bit information from status signal - this door instanc is communicating*)
END_VAR


(*Group:DOORS_in*)


VAR_INPUT
	XSwVer :	USINT;(*System software version; Incremented if new SW is incompatible to previous SW version or hardware.; Default : 0*)
	XSwRel :	USINT;(*System software release;  Incremented if new SW has major changes or additional functions but is still compatible.; Default : 0*)
	XSwUpd :	USINT;(*System software update; Incremented if new SW with minor modifications during elaboration ; Default : 0*)
	XSwEvo :	USINT;(*System software evolution; Incremented if new SW is for testing (not validated); Default : 0*)
	XHwVer :	USINT;(*System hardware version ; Basic functional changes, incompatible to previous HW versions (e.g. change in firmware, I/O). ; Default : 0*)
	XHwRel :	USINT;(*System hardware release ; Minor changes. Still compatible; Default : 0*)
	XLifeSign_1 :	INT;(*System shall set this counter to zero after reset including power-up and increment it with every telegram cycle.*)
	XLifeSign_2 :	INT;(*System shall set this counter to zero after reset including power-up and increment it with every telegram cycle.*)
	XDevAppMd :	USINT;(*Device application mode*)
	XDevCfg :	USINT;(*Device configuration*)
	XMd :	USINT;(*Door mode. Default : 1*)
	XSts :	USINT;(*Door Status; Clarification of Signals:; Bit 0: Door Closed & Locked (either released or not released); Bit 1: Door is inhibited due to a PED failure; Bit 2: Door is inhibited due to SDO ; Bit 3: Door is inhibited by the Driver; Bit 4: Door is either open or closed and is released.; Bit 5: Door is not in closed position; Bit 6: Door has been unlocked using either EED or EAD; Default : 0*)
	SEmEgrDevOpr :	BOOL;(*Emergency Egress Device Operated*)
	SEmAccss :	BOOL;(*Emergency Access Device Operated*)
	SLkdOutOfSrvc :	BOOL;(*Door Locked Out of Service*)
	SFltA :	BOOL;(*Door Fault - priority A; Summary fault signal in case any priority A diagnostic code is set in the EDCU; Used as the signal to inform the driver when to isolate the door from opening*)
	SFltB :	BOOL;(*Door Fault - priority B; Summary fault signal in case any priority B diagnostic code is set in the EDCU*)
	SOpSqAct :	BOOL;(*Door Opening sequence active.*)
	SClSwAct :	BOOL;(*Door Closing sequence active.*)
	STrnAtSpdFb :	BOOL;(*Feedback of Input 3; (Train at Speed)*)
	SRelFb :	BOOL;(*Feedback of Input 4; (Door Release)*)
	SClFb :	BOOL;(*Feedback of Input 5; (Door Close)*)
	SOpPb :	BOOL;(*Door Open Pushbutton*)
	SObstDet :	BOOL;(*Obstacle detected ; Signal is set TRUE as soon as an obstacle is detected either during opening or closing sequence ; Signal is reset to FALSE when door moves to Closed & Locked position.*)
	SBurninAct :	BOOL;(*Burn In test (door cycling) is active.*)
	SBurninOk :	BOOL;(*Burn In test (door cycling) passed.; Last Burn in test was successful: door cycling completed without any failure detected.*)
	SBurninNotOk :	BOOL;(*Burn In test (door cycling) not passed.; Last Burn in test was not successful: door cycling not completed correctly (aborted or door fault detected).*)
	SCrewSwOp :	BOOL;(*Crew key switch open is operated. Signal type: Steady signal.*)
	SClLckFlt :	BOOL;(*Door leaves the closed & locked position without permission.*)
	SSrvcPbFlt :	BOOL;(*Service Button on the DCU fails for a duration of 1 minute.*)
	SBusComFlt :	BOOL;(*Bus communication fails ; Signals and functions via bus are no longer available*)
	SSdoEnFlt :	BOOL;(*SDO Enable Hardware & SDO Enable Software Mismatch 1; H/W SDO Enable =1 (I01) & S/W SDO Enable = 0 for 5s; OR; H/W SDO Enable =0 (I01) & S/W SDO Enable = 1 for 5s*)
	SClFlt :	BOOL;(*Door Closed HW & Door Close SW commands mismatch; - H/W close input (I05) changes to TRUE and S/W close signal is not detected TRUE within 5sec; OR;  - S/W close signal changes to TRUE and H/W close input (I05) is not detected TRUE within 5sec*)
	SOpPosFlt :	BOOL;(*System does not detect opened position within a defined time*)
	SClPosFlt :	BOOL;(*System does not detect closed position within a defined time*)
	STrnCtrlFlt :	BOOL;(*SIgnals from the Train Control are faulty; Door release signal active (I04) and Train at speed*)
END_VAR


(*Group: OUT to generic SW (consist level)*)


VAR_OUTPUT
	PAD_DcuStatus :	PadGDcuStatus;(*status from door in car*)
	PAD_DcuStatusV :	BOOL;(*status from door  in car  valid bit*)
	PAD_DcuStatusExt :	PadFDcuStatusExt;(*status from door in car*)
END_VAR


