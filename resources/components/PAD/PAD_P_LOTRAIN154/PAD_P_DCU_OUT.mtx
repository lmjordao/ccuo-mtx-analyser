 <interface>
  <variable-group name="Local">
   <variable name="Enable_FA" array-size="1" type="MWT_BOOL" usage="input">
    <comment><![CDATA[Enable PAD FA]]></comment>
   </variable>
  </variable-group>
  <variable-group name="Input (from generic SW)">
   <variable name="PAD_DcuCmd_01" array-size="1" type="PadGDcuCmd" usage="input">
    <comment><![CDATA[commands to door 01]]></comment>
   </variable>
   <variable name="PAD_DcuCmd_02" array-size="1" type="PadGDcuCmd" usage="input">
    <comment><![CDATA[commands to door 02]]></comment>
   </variable>
   <variable name="PAD_DcuCmd_05" array-size="1" type="PadGDcuCmd" usage="input">
    <comment><![CDATA[commands to door 05]]></comment>
   </variable>
   <variable name="PAD_DcuCmd_06" array-size="1" type="PadGDcuCmd" usage="input">
    <comment><![CDATA[commands to door 06]]></comment>
   </variable>
   <variable name="CarId" array-size="1" type="MWT_USINT" usage="input">
    <comment><![CDATA[car Id]]></comment>
   </variable>
  </variable-group>
  <variable-group name="Input (project specific)">
   <variable name="PAD_SDrTrmMdCst" array-size="1" type="MWT_BOOL" usage="input">
    <comment><![CDATA[Terminal Station Mode Active in consist]]></comment>
   </variable>
   <variable name="PAD_CCrewSwitchAct" array-size="1" type="MWT_BOOL" usage="input">
    <comment><![CDATA[Activates / deactivates the crew switch (0: active, 1: not active)]]></comment>
   </variable>
   <variable name="PAD_CarOut" array-size="1" type="PadFCarOut" usage="input">
    <comment><![CDATA[Family : Consist : All data related to a car to other HLFG as Output.]]></comment>
   </variable>
  </variable-group>
  <variable-group name="CONST"/>
  <variable-group name="Instance"/>
  <variable-group name="DOORS_out">
   <variable name="XLifeSign" array-size="1" type="MWT_UINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XLifeSign">
    <comment><![CDATA[CCU life sign; CCU shall set this counter to zero after reset including power-up and increment it with every application cycle. ; Subscriber: all systems]]></comment>
   </variable>
   <variable name="SAutClInh" array-size="1" type="MWT_BOOL" usage="output" connect-name="CO_&lt;car&gt;DCU_SAutClInh">
    <comment><![CDATA[Auto Close Inhibit; Will be set when the Auto Close function is to be inhibited. Used for when train is interfacing with platform screen doors or in case of fire/smoke is detected on the train.; Default : 0]]></comment>
   </variable>
   <variable name="SMntMdAct" array-size="1" type="MWT_BOOL" usage="output" connect-name="CO_&lt;car&gt;DCU_SMntMdAct">
    <comment><![CDATA[Maintenance Mode Active. ; Will be set when train is in maintenance mode.; (Adjusts Auto Close time to 5 sec, precondition to enable cycling mode); Default : 0]]></comment>
   </variable>
   <variable name="CRelOp" array-size="1" type="MWT_BOOL" usage="output" connect-name="CO_&lt;car&gt;DCU_CRelOp">
    <comment><![CDATA[Door Release/Open; Tells the door system to either release and open or release and not open when a valid door release command is received.; Default : 1]]></comment>
   </variable>
   <variable name="ZOp" array-size="1" type="MWT_USINT" usage="output">
    <comment><![CDATA[Door Open; Global open command to all doors to open when the door is released and not open. This signal is from the TCMS HMI Open Button not from the hardware open buttons.; Default : 0;                           (pulsed)]]></comment>
   </variable>
   <variable name="ZCl" array-size="1" type="MWT_USINT" usage="output">
    <comment><![CDATA[Close Door; Tells the door to close and lock and remove release.; Default : 0,                <decode>;                           (pulsed)]]></comment>
   </variable>
   <variable name="SClrMd" array-size="1" type="MWT_BOOL" usage="output" connect-name="CO_&lt;car&gt;DCU_SClrMd">
    <comment><![CDATA[Train Clearance Mode; Default : 0]]></comment>
   </variable>
   <variable name="XDrvrInh" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XDrvrInh">
    <comment><![CDATA[Driver Inhibit; Stops the door from opening when the driver has selected Door Inhibit from the HMI; Default: 0,                <decode>]]></comment>
   </variable>
   <variable name="XNotSrvc" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XNotSrvc">
    <comment><![CDATA[Out of Service Indicator; Tells the door to illuminate the out of service indicator. ; Default: 0,                <decode>]]></comment>
   </variable>
   <variable name="XAutClTm" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XAutClTm">
    <comment><![CDATA[Auto Close Time; Global parameter for all doors defining what the auto-close time is.; Default : 45]]></comment>
   </variable>
   <variable name="XCclCmd" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XCclCmd">
    <comment><![CDATA[Doors Cycle Command; Tells the doors to cycle open/close for a defined number of cycles (parameter adjustable by TCMS). ; Steady Signal.; Cycling stops if signal goes low.; Default : 0               <decode>]]></comment>
   </variable>
   <variable name="XNoOfObstAttmpt" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XNoOfObstAttmpt">
    <comment><![CDATA[No. Obstacles Re-open or Re-close attempts; (Global signal which is the same for all doors); Default : 3]]></comment>
   </variable>
   <variable name="XReOpDist" array-size="1" type="MWT_UINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XReOpDist">
    <comment><![CDATA[Re-opening Distance; Re-opening width on door obstacle detection; Default : 100]]></comment>
   </variable>
   <variable name="XNoOfCcls" array-size="1" type="MWT_UINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XNoOfCcls">
    <comment><![CDATA[No. Door Cycles; No. of cycles to be performed by the Door when 'Doors Cycle Command' is active; (Invalid values shall be interpreted as '0'); Default : 0]]></comment>
   </variable>
   <variable name="XClPbInh" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XClPbInh">
    <comment><![CDATA[Description: inhibits function of push button close when active. Signal type: steady signal. Default: 0]]></comment>
   </variable>
   <variable name="XtAutCl" array-size="1" type="MWT_USINT" usage="output">
    <comment><![CDATA[Auto Close Time when open with Crew key (Lotrain only).; Global parameter for all doors defining what the auto-close time is.; Lost comm : Last value received even lost power; Default: 45]]></comment>
   </variable>
   <variable name="XRelEn" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XRelEn">
    <comment><![CDATA[Release Enable; Allows the door to release when a valid hardware release command is received; Default: 0,                <decode>]]></comment>
   </variable>
   <variable name="XSdoEn" array-size="1" type="MWT_USINT" usage="output" connect-name="CO_&lt;car&gt;DCU_XSdoEn">
    <comment><![CDATA[SDO Enable; Allows the SDO door to release (combined with the HW SDO enable) when a valid hardware release command is received; Default: 0,                <decode>]]></comment>
   </variable>
   <variable name="CCrewSwitchAct" array-size="1" type="MWT_BOOL" usage="output">
    <comment><![CDATA[Crew Switch not active; Activates / deactivates the crew switch; default: 0]]></comment>
   </variable>
  </variable-group>
 </interface>
</pou>
